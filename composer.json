{
  "require": {
    "php": ">=5.6.4",
    "robmorgan/phinx": "^0.9.1",
    "symfony/filesystem": "^3.2",
    "symfony/finder": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "fzaninotto/faker": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Exceptions\\": "assets/Exceptions/",
      "Lib\\": "assets/php-lib/",
      "Lib\\MusicClasses\\": "assets/php-lib/MusicClasses/",
      "Providers\\": "assets/Providers/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
